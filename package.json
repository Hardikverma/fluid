{
  "name": "custom-video-player",
  "version": "1.0.0",
  "description": "HTML5 video player",
  "main": "src/index.js",
  "scripts": {
    "test": "echo \"Error: no test specified\" && exit 1",
    "build": "webpack --mode=production && uglifyjs dist/player.min.js -o dist/player.min.js -c -m",
    "build-dev": "webpack --mode=development",
    "build-obf": "webpack --mode=production --env obf=true && uglifyjs dist/player.min.js -o dist/player.min.js -c -m",
    "build-cdn": "webpack --mode=production --env dist=current --env obf=true && uglifyjs dist/player.min.js -o dist/player.min.js -c -m",
    "build-dev-debug": "webpack --mode=development --env debug",
    "lint": "eslint src --ext .js",
    "lint-fix": "eslint src --ext .js --fix",
    "dev-server": "webpack-dev-server --mode=development --host 0.0.0.0",
    "dev-server-debug": "webpack-dev-server --mode=development --env debug --host 0.0.0.0",
    "dev-server-test": "webpack-dev-server --mode=production --host 0.0.0.0"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/Joel2B/custom-video-player.git"
  },
  "author": "JoelB",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/Joel2B/custom-video-player/issues"
  },
  "homepage": "https://appsdev.cyou",
  "browserslist": [
    "> 0.25%",
    "not dead",
    "IE 11"
  ],
  "dependencies": {
    "es6-promise": "^4.2.8",
    "uglify-js": "^3.13.10",
    "videojs-vtt.js": "~0.15.3"
  },
  "devDependencies": {
    "@babel/core": "^7.14.6",
    "@babel/preset-env": "^7.14.7",
    "babel-eslint": "^10.1.0",
    "babel-loader": "^8.2.2",
    "cheerio": "1.0.0-rc.10",
    "copy-webpack-plugin": "^9.0.1",
    "css-loader": "^6.1.0",
    "eslint-config-standard": "^16.0.3",
    "eslint-plugin-import": "^2.25.2",
    "eslint-plugin-node": "^11.1.0",
    "eslint-plugin-promise": "^5.1.0",
    "html-webpack-plugin": "^5.3.2",
    "prettier-eslint": "^13.0.0",
    "scriptjs": "^2.5.9",
    "semver": "^7.3.5",
    "style-loader": "^3.1.0",
    "svg-url-loader": "^7.1.1",
    "webpack": "^5.45.1",
    "webpack-cli": "^4.7.2",
    "webpack-dev-server": "^3.11.2",
    "webpack-obfuscator": "^3.3.2"
  }
}
